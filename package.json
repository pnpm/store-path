{
  "name": "@pnpm/store-path",
  "version": "2.0.0",
  "description": "Resolves the pnpm store path",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "prepublishOnly": "npm run tsc",
    "tsc": "rimraf lib && tsc",
    "lint": "tslint -c tslint.json --project .",
    "test": "npm run lint && preview && ts-node test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pnpm/store-path.git"
  },
  "keywords": [
    "pnpm",
    "store"
  ],
  "engines": {
    "node": ">=8.15"
  },
  "author": "Zoltan Kochan <z@kochan.io> (https://www.kochan.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pnpm/store-path/issues"
  },
  "homepage": "https://github.com/pnpm/store-path#readme",
  "dependencies": {
    "can-link": "1.0.2",
    "mkdirp-promise": "5.0.1",
    "mz": "2.7.0",
    "path-absolute": "1.0.1",
    "path-temp": "2.0.0",
    "root-link-target": "2.0.0",
    "touch": "3.1.0"
  },
  "devDependencies": {
    "@types/node": "^12.0.0",
    "@types/rimraf": "^2.0.2",
    "@types/tape": "^4.2.33",
    "@types/touch": "^3.1.1",
    "mos": "^2.0.0-alpha.3",
    "mos-plugin-readme": "^1.0.4",
    "package-preview": "^2.0.0",
    "rimraf": "^2.6.3",
    "tape": "^4.10.1",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5"
  },
  "mos": {
    "plugins": [
      "readme"
    ],
    "installation": {
      "useShortAlias": true
    }
  }
}
